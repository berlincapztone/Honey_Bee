{
  "builds": [
    {
      "src": "honeypro/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "honeypro/wsgi.py"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",    
      "destination": "/index.html"  
    }
  ]
}
